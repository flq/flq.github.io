---
import type { CollectionEntry } from "astro:content";

interface Props {
    entry: CollectionEntry<"blog">;
}

const { entry } = Astro.props;
const {
    remarkPluginFrontmatter: { excerpt },
} = await entry.render();
const {
    data: { title, date },
    slug,
} = entry;
---
<li><a href={`/posts/${slug}`}><h3>{title}</h3><p>{excerpt}</p></a></li>